  "dependencies": [
    {
      "name": "ajjahn/samba",
      "version_requirement": ">= 0.5.0"
    },{
      "name": "derdanne/nfs",
      "version_requirement": ">= 1.0.2"
    },{
      "name": "puppet/autofs",
      "version_requirement": ">= 1.4.4"
    },{
      "name": "lex/dnsmasq",
      "version_requirement": ">= 2.6.1"
    },{
      "name": "puppet/staging",
      "version_requirement": ">= 2.1.0"
    },{
      "name": "puppet/nginx",
      "version_requirement": ">= 0.6.0"
    },{
      "name": "puppet/squid",
      "version_requirement": ">= 0.6.0"
    },{
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1"
